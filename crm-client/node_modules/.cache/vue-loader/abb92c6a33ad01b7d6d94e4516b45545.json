          />\n          <form-select\n            :item=\"item\"\n            :errors=\"errors\"\n            property=\"type\"\n            option-store=\"projectType\"\n            label=\"type_name\"\n            @formUpdated=\"updateValue\"\n          />\n          <form-select\n            :item=\"item\"\n            :errors=\"errors\"\n            property=\"status\"\n            option-store=\"projectStatus\"\n            label=\"status_name\"\n            @formUpdated=\"updateValue\"\n          />\n          <form-select-autocomplete\n            :item=\"item\"\n            :errors=\"errors\"\n            property=\"client\"\n            option-store=\"client\"\n            label=\"client_name\"\n            @formUpdated=\"updateValue\"\n          />\n          <form-checkbox\n            :item=\"item\"\n            :errors=\"errors\"\n            property=\"isActive\"\n            label=\"isActive\"\n            @formUpdated=\"updateValue\"\n          />\n        </div>\n      </div>\n      <item-edit-actions :item=\"item\" entity=\"Project\" path=\"project\" />\n    </section>\n  </form>\n</template>\n\n<script>\nimport { mapActions, mapGetters } from 'vuex'\nimport ItemEditActions from '../../components/layout/ItemEditActions'\nimport ItemErrors from '../../components/layout/errors/ItemErrors'\n\nexport default {\n  components: {\n    ItemErrors,\n    ItemEditActions\n  },\n  props: {\n    handleSubmit: {\n      type: Function,\n      required: true\n    },\n    item: {\n      type: Object,\n      required: true\n    }\n  },\n  computed: {\n    ...mapGetters({\n      errors: 'project/errors'\n    })\n  },\n  beforeDestroy() {\n    this.reset()\n  },\n  methods: {\n    ...mapActions({\n      reset: 'project/reset'\n    }),\n    updateValue(property, value) {\n      this.$store.commit('project/PROJECT_UPDATE_ITEM', { [property]: value })\n    }\n  }\n}\n</script>\n"]}]}