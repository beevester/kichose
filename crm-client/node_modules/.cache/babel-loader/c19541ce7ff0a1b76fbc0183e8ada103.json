,iBAAA,MAAA,CAAA,YAAA,GAAA,IAAA;AAAA,SAAA,EAAA,EAAA,CAAA;AACA,OAJA;;AAMA,UAAA,KAAA,YAAA,EAAA;AACA,aAAA,YAAA,GAAA,KAAA;AACA,QAAA,UAAA,CAAA,IAAA,EAAA,EAAA,CAAA;AACA,OAHA,MAGA;AACA,QAAA,IAAA;AACA;;AAEA,MAAA,WAAA,CAAA,eAAA;AACA,KA/CA;AAgDA,IAAA,WAhDA,8BAgDA;AAAA,UAAA,KAAA,SAAA,KAAA;AAAA,UAAA,GAAA,SAAA,GAAA;AACA,WAAA,IAAA,CAAA,MAAA,CAAA,MAAA,CAAA,KAAA,CAAA,IAAA,EAAA,MAAA,CAAA,KAAA,CAAA;AACA,WAAA,KAAA,GAAA,KAAA;AACA,WAAA,GAAA,GAAA,GAAA;AACA;AApDA;AA5DA,CAAA","sourcesContent":["<template>\n  <is-main-template title=\"calendar\">\n    <v-row>\n      <v-col cols=\"12\">\n        <v-sheet height=\"64\">\n          <v-toolbar flat color=\"white\">\n            <v-btn class=\"mr-4\" @click=\"setToday\">\n              Today\n            </v-btn>\n            <v-btn fab text small class=\"mx-1\" @click=\"prev\">\n              <v-icon small>arrow_back_ios</v-icon>\n            </v-btn>\n            <v-btn fab text small class=\"mx-1\" @click=\"next\">\n              <v-icon small>arrow_forward_ios</v-icon>\n            </v-btn>\n            <v-toolbar-title class=\"ml-4\">{{ title }}</v-toolbar-title>\n            <v-spacer></v-spacer>\n            <v-menu bottom right>\n              <template v-slot:activator=\"{ on }\">\n                <v-btn v-on=\"on\">\n                  <span>{{ typeToLabel[type] }}</span>\n                  <v-icon right>arrow_drop_down</v-icon>\n                </v-btn>\n              </template>\n              <v-list>\n                <v-list-item @click=\"type = 'day'\">\n                  <v-list-item-title>Day</v-list-item-title>\n                </v-list-item>\n                <v-list-item @click=\"type = 'week'\">\n                  <v-list-item-title>Week</v-list-item-title>\n                </v-list-item>\n                <v-list-item @click=\"type = 'month'\">\n                  <v-list-item-title>Month</v-list-item-title>\n                </v-list-item>\n              </v-list>\n            </v-menu>\n          </v-toolbar>\n        </v-sheet>\n        <v-sheet height=\"600\">\n          <v-calendar\n            ref=\"calendar\"\n            v-model=\"focus\"\n            color=\"primary\"\n            :events=\"events\"\n            :event-color=\"getEventColor\"\n            :event-margin-bottom=\"3\"\n            :now=\"today\"\n            :type=\"type\"\n            @click:event=\"showEvent\"\n            @click:more=\"viewDay\"\n            @click:date=\"viewDay\"\n            @change=\"updateRange\"\n          ></v-calendar>\n          <v-menu\n            v-model=\"selectedOpen\"\n            :close-on-content-click=\"false\"\n            :activator=\"selectedElement\"\n            full-width\n            offset-x\n          >\n            <v-card color=\"grey lighten-4\" min-width=\"350px\" flat>\n              <v-toolbar :color=\"selectedEvent.color\" dark>\n                <v-btn\n                  icon\n                  :to=\"{ name: 'TaskUpdate', params: { id: selectedEvent.id } }\"\n                >\n                  <v-icon>edit</v-icon>\n                </v-btn>\n                <v-toolbar-title v-html=\"selectedEvent.name\"></v-toolbar-title>\n              </v-toolbar>\n              <v-card-text>\n                <p>{{ crmDateFormat(selectedEvent.deadline, 'YYYY-MM-DD') }}</p>\n                <p>{{ selectedEvent.description }}</p>\n                <p>\n                  {{ $t('client') }}:\n                  <router-link\n                    v-if=\"\n                      selectedEvent &&\n                        selectedEvent.project &&\n                        selectedEvent.project.client\n                    \"\n                    :to=\"{\n                      name: 'ClientShow',\n                      params: { id: selectedEvent.project.client.id }\n                    }\"\n                  >\n                    {{ selectedEvent.project.client.name }}\n                  </router-link>\n                </p>\n                <p>\n                  {{ $t('assignee_name') }}:\n                  <router-link\n                    v-if=\"selectedEvent && selectedEvent.assignee\"\n                    :to=\"{\n                      name: 'UserShow',\n                      params: { id: selectedEvent.assignee.id }\n                    }\"\n                  >\n                    {{ selectedEvent.assignee.name }}\n                  </router-link>\n                </p>\n              </v-card-text>\n              <v-card-actions>\n                <v-btn\n                  text\n                  color=\"success\"\n                  :to=\"{ name: 'TaskShow', params: { id: selectedEvent.id } }\"\n                >\n                  More details\n                </v-btn>\n                <v-btn text color=\"secondary\" @click=\"selectedOpen = false\">\n                  Close\n                </v-btn>\n              </v-card-actions>\n            </v-card>\n          </v-menu>\n        </v-sheet>\n      </v-col>\n    </v-row>\n  </is-main-template>\n</template>\n\n<script>\nimport { mapActions, mapGetters } from 'vuex'\n\nexport default {\n  data() {\n    return {\n      today: window.moment().format('YYYY-MM-DD'),\n      focus: window.moment().format('YYYY-MM-DD'),\n      type: 'month',\n      typeToLabel: {\n        month: 'Month',\n        week: 'Week',\n        day: 'Day'\n      },\n      start: null,\n      end: null,\n      selectedEvent: {},\n      selectedElement: null,\n      selectedOpen: false\n    }\n  },\n  computed: {\n    ...mapGetters({\n      tasks: 'task/items'\n    }),\n    events() {\n      this.tasks.forEach(task => {\n        task.start = this.crmDateFormat(task.deadline, 'YYYY-MM-DD')\n        task.end = this.crmDateFormat(task.deadline, 'YYYY-MM-DD')\n        task.color = 'primary'\n      })\n\n      return this.tasks\n    },\n    title() {\n      const { start, end } = this\n      if (!start || !end) {\n        return ''\n      }\n\n      const startMonth = this.monthFormatter(start)\n      const startYear = start.year\n      const startDay = start.day\n\n      switch (this.type) {\n        case 'month':\n          return `${startMonth} ${startYear}`\n        case 'week':\n        case 'day':\n          return `${startMonth} ${startDay} ${startYear}`\n      }\n      return ''\n    },\n    monthFormatter() {\n      return this.$refs.calendar.getFormatter({\n        timeZone: 'UTC',\n        month: 'long'\n      })\n    }\n  },\n  created() {\n    this.load(window.moment().format('M/Y'))\n  },\n  methods: {\n    ...mapActions({\n      getTasks: 'task/getItems'\n    }),\n    load(date) {\n      this.getTasks({\n        'deadline[after]': window.moment(date, 'M/Y').format('DD-MM-YYYY'),\n        'deadline[before]': window.moment(date, 'M/Y')\n          .endOf('month')\n          .add(1, 'day')\n          .format('DD-MM-YYYY'),\n        'order[deadline]': 'asc',\n        'status.id[]': '1',\n        itemsPerPage: '500'\n      })\n    },\n    viewDay({ date }) {\n      this.focus = date\n      this.type = 'day'\n    },\n    getEventColor(event) {\n      return event.color\n    },\n    setToday() {\n      this.focus = this.today\n    },\n    prev() {\n      this.$refs.calendar.prev()\n    },\n    next() {\n      this.$refs.calendar.next()\n    },\n    showEvent({ nativeEvent, event }) {\n      const open = () => {\n        this.selectedEvent = event\n        this.selectedElement = nativeEvent.target\n        setTimeout(() => (this.selectedOpen = true), 10)\n      }\n\n      if (this.selectedOpen) {\n        this.selectedOpen = false\n        setTimeout(open, 10)\n      } else {\n        open()\n      }\n\n      nativeEvent.stopPropagation()\n    },\n    updateRange({ start, end }) {\n      this.load(window.moment(start.date).format('M/Y'))\n      this.start = start\n      this.end = end\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\">\n.theme--light {\n  .v-calendar {\n    &-weekly {\n      &__day-label {\n        margin: 4px 0 !important;\n      }\n      &__head-weekday {\n        flex: 1 0 0;\n        border-top: 1px solid #e0e0e0;\n        border-bottom: 1px solid #e0e0e0;\n        &:first-child {\n          border-left: 1px solid #e0e0e0;\n        }\n      }\n      &__day {\n        &:first-child {\n          border-left: 1px solid #e0e0e0;\n        }\n      }\n    }\n    .v-event {\n      &-end {\n        margin: 0 auto;\n      }\n    }\n  }\n}\n</style>\n"],"sourceRoot":"src/pages"}]}