IA,MAJA,EAIA;AACA,aAAA,IAAA,CAAA,IAAA,IAAA,MAAA,CAAA,KAAA;AACA,KANA;AAOA,IAAA,cAPA,0BAOA,IAPA,EAOA,MAPA,EAOA;AACA,aAAA,IAAA,CAAA,cAAA,IAAA,MAAA,CAAA,cAAA,IAAA,MAAA;AACA,KATA;AAUA,IAAA,SAVA,qBAUA,IAVA,EAUA,MAVA,EAUA;AACA,UAAA,IAAA,CAAA,IAAA,IAAA,IAAA,CAAA,IAAA,CAAA,KAAA,EAAA;AACA,eAAA,IAAA,CAAA,IAAA,CAAA,KAAA;AACA;;AAEA,UAAA,MAAA,CAAA,IAAA,IAAA,MAAA,CAAA,IAAA,CAAA,KAAA,EAAA;AACA,eAAA,MAAA,CAAA,IAAA,CAAA,KAAA;AACA;AACA,KAlBA;AAmBA,IAAA,UAnBA,sBAmBA,IAnBA,EAmBA,MAnBA,EAmBA;AACA,UAAA,KAAA,GAAA,IAAA;;AAEA,UAAA,IAAA,CAAA,IAAA,IAAA,IAAA,CAAA,IAAA,CAAA,KAAA,EAAA;AACA,QAAA,KAAA,GAAA,IAAA,CAAA,IAAA,CAAA,KAAA;AACA;;AAEA,UAAA,MAAA,CAAA,IAAA,IAAA,MAAA,CAAA,IAAA,CAAA,KAAA,EAAA;AACA,QAAA,KAAA,GAAA,MAAA,CAAA,IAAA,CAAA,KAAA;AACA;;AAEA,aAAA,KAAA;AACA,KA/BA;AAgCA,IAAA,IAhCA,gBAgCA,MAhCA,EAgCA;AACA,4BAAA,MAAA,CAAA,KAAA;AACA,KAlCA;AAmCA,IAAA,KAnCA,iBAmCA,IAnCA,EAmCA,MAnCA,EAmCA;AACA,aAAA,KAAA,IAAA,CAAA,KAAA,IAAA,CAAA,IAAA,EAAA,MAAA,CAAA,EAAA,IAAA,CAAA;AACA,KArCA;AAsCA,IAAA,IAtCA,gBAsCA,GAtCA,EAsCA,IAtCA,EAsCA;AACA,aAAA,KAAA,IAAA,CAAA,IAAA,CAAA,GAAA,EAAA,IAAA,CAAA;AACA;AAxCA;AApBA,CAAA","sourcesContent":["<template>\n  <v-data-table\n    :headers=\"headers\"\n    :items=\"items\"\n    :hide-default-footer=\"hideDefaultFooter\"\n    :items-per-page=\"itemsPerPage\"\n    :footer-props=\"{\n      itemsPerPageOptions: [5, 10, 25, 50, 100]\n    }\"\n  >\n    <template\n      v-for=\"(header, h) in headers\"\n      v-slot:[name(header)]=\"{ item, header }\"\n    >\n      <template v-if=\"type(item, header) === 'boolean'\">\n        <v-chip\n          :key=\"h\"\n          :color=\"value(item, header) ? 'green' : 'red'\"\n          text-color=\"white\"\n        >\n          {{ value(item, header) ? $t('yes') : $t('no') }}\n        </v-chip>\n      </template>\n\n      <template v-else-if=\"type(item, header) === 'datetime'\">\n        {{ crmDateFormat(value(item, header)) }}\n      </template>\n\n      <template v-else-if=\"type(item, header) === 'image'\">\n        <img\n          :src=\"value(item, header)\"\n          alt=\"\"\n          :width=\"item.params.width\"\n          class=\"mt-2\"\n        />\n      </template>\n\n      <template v-else-if=\"type(item, header) === 'collection'\">\n        <span v-for=\"(obj, key) in value(item, header)\" :key=\"key\" class=\"mr-3\">\n          <template v-if=\"item.link || header.link\">\n            <v-btn\n              :to=\"{\n                name: routeName(item, header),\n                params: { id: pick(routeParam(item, header), obj) }\n              }\"\n              class=\"px-1\"\n              color=\"primary\"\n              text\n            >\n              {{ pick(collectionPath(item, header), obj) }}\n            </v-btn>\n          </template>\n          <template v-else>\n            <v-chip color=\"gray\">\n              {{ pick(collectionPath(item, header), obj) }}\n            </v-chip>\n          </template>\n        </span>\n      </template>\n\n      <template v-else-if=\"type(item, header) === 'list'\">\n        <ul :key=\"h\" class=\"my-2\">\n          <li v-for=\"(val, key) in item.data\" :key=\"key\">\n            <b>{{ $t(key) }}:</b> {{ val }}\n          </li>\n        </ul>\n      </template>\n\n      <template v-else>\n        <template v-if=\"item.link || header.link\">\n          <v-btn\n            v-if=\"value(item, header)\"\n            :key=\"h\"\n            :to=\"{\n              name: routeName(item, header),\n              params: { id: pick(routeParam(item, header), item) }\n            }\"\n            class=\"px-1\"\n            color=\"primary\"\n            text\n          >\n            {{ value(item, header) }}\n          </v-btn>\n        </template>\n        <template v-else>\n          {{ value(item, header) }}\n        </template>\n      </template>\n    </template>\n\n    <template v-for=\"(_, slot) of $scopedSlots\" v-slot:[slot]=\"scope\"\n      ><slot :name=\"slot\" v-bind=\"scope\"\n    /></template>\n  </v-data-table>\n</template>\n\n<script>\nexport default {\n  name: 'IsTable',\n  props: {\n    headers: {\n      type: Array,\n      required: true\n    },\n    items: {\n      type: Array,\n      required: true\n    },\n    hideDefaultFooter: {\n      type: Boolean,\n      default: false\n    },\n    itemsPerPage: {\n      type: Number,\n      default: 50\n    }\n  },\n  methods: {\n    type(item, header) {\n      return item.propertyType || header.type\n    },\n    path(item, header) {\n      return item.path || header.value\n    },\n    collectionPath(item, header) {\n      return item.collectionPath || header.collectionPath || 'name'\n    },\n    routeName(item, header) {\n      if (item.link && item.link.route) {\n        return item.link.route\n      }\n\n      if (header.link && header.link.route) {\n        return header.link.route\n      }\n    },\n    routeParam(item, header) {\n      let param = 'id'\n\n      if (item.link && item.link.param) {\n        param = item.link.param\n      }\n\n      if (header.link && header.link.param) {\n        param = header.link.param\n      }\n\n      return param\n    },\n    name(header) {\n      return `item.${header.value}`\n    },\n    value(item, header) {\n      return this.pick(this.path(item, header), item)\n    },\n    pick(key, item) {\n      return this.$dot.pick(key, item)\n    }\n  }\n}\n</script>\n"],"sourceRoot":"src/components/layout"}]}