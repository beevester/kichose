ions({\n      getItem: 'group/getItem',\n      reset: 'group/reset',\n      getModules: 'module/getItems'\n    }),\n    roles(module) {\n      return this.item.roles.filter(role => role.module.id === module.id)\n    }\n  }\n}\n</script>\n"],"sourceRoot":"src/pages/group"}]}