,EAAA,QAAA,oBACA,UAAA,CAAA;AACA,IAAA,MAAA,EAAA;AADA,GAAA,CADA,CAfA;AAoBA,EAAA,aApBA,2BAoBA;AACA,SAAA,KAAA;AACA,GAtBA;AAuBA,EAAA,OAAA,oBACA,UAAA,CAAA;AACA,IAAA,KAAA,EAAA;AADA,GAAA,CADA;AAIA,IAAA,WAJA,uBAIA,QAJA,EAIA,KAJA,EAIA;AACA,WAAA,MAAA,CAAA,MAAA,CAAA,0CAAA,sBACA,QADA,EACA,KADA;AAGA,KARA;AASA,IAAA,OATA,qBASA;AACA,WAAA,IAAA,CAAA,KAAA,CAAA,IAAA,CAAA;AAAA,QAAA,IAAA,EAAA,IAAA,CAAA,GAAA;AAAA,OAAA;AACA,KAXA;AAYA,IAAA,UAZA,sBAYA,IAZA,EAYA;AACA,WAAA,IAAA,CAAA,KAAA,GAAA,KAAA,IAAA,CAAA,KAAA,CAAA,MAAA,CAAA,UAAA,EAAA,EAAA;AACA,YAAA,CAAA,IAAA,CAAA,EAAA,EAAA;AACA,UAAA,IAAA,CAAA,EAAA,GAAA,IAAA,CAAA,IAAA;AACA;;AAEA,eAAA,IAAA,CAAA,EAAA,KAAA,EAAA,CAAA,EAAA;AACA,OANA,CAAA;AAOA,KApBA;AAqBA,IAAA,eArBA,2BAqBA,IArBA,EAqBA;AACA,UAAA,CAAA,IAAA,CAAA,IAAA,EAAA;AACA,QAAA,IAAA,CAAA,IAAA,GAAA,IAAA,CAAA,OAAA,CAAA,IAAA;AACA;;AAEA,UAAA,CAAA,IAAA,CAAA,GAAA,EAAA;AACA,QAAA,IAAA,CAAA,GAAA,GAAA,IAAA,CAAA,OAAA,CAAA,GAAA;AACA;;AAEA,UAAA,CAAA,IAAA,CAAA,UAAA,EAAA;AACA,QAAA,IAAA,CAAA,UAAA,GAAA,CAAA;AACA,OAXA,CAaA;;;AACA,UAAA,CAAA,IAAA,CAAA,cAAA,EAAA,CACA;AACA;AACA;AAtCA;AAvBA,CAAA","sourcesContent":["<template>\n  <form @submit.prevent=\"handleSubmit(item)\">\n    <section class=\"content\">\n      <item-errors entity=\"invoiceHeader\" />\n      <div class=\"box box-primary\">\n        <div class=\"box-header with-border\">\n          <h3 class=\"box-title\">\n            {{ $t('tabs_general') }}\n          </h3>\n        </div>\n        <div class=\"box-body\">\n          <div class=\"row\">\n            <div class=\"col-md-6\">\n              <form-input\n                :item=\"item\"\n                :errors=\"errors\"\n                property=\"number\"\n                label=\"number\"\n                @formUpdated=\"updateValue\"\n              />\n            </div>\n            <div class=\"col-md-6\">\n              <form-select\n                :item=\"item\"\n                :errors=\"errors\"\n                property=\"status\"\n                option-store=\"invoiceStatus\"\n                label=\"status_name\"\n                @formUpdated=\"updateValue\"\n              />\n            </div>\n          </div>\n          <div class=\"row\">\n            <div class=\"col-md-6\">\n              <form-select\n                :item=\"item\"\n                :errors=\"errors\"\n                property=\"companyFrom\"\n                option-store=\"company\"\n                label=\"companyFrom_name\"\n                @formUpdated=\"updateValue\"\n              />\n            </div>\n            <div class=\"col-md-6\">\n              <form-select\n                :item=\"item\"\n                :errors=\"errors\"\n                property=\"companyTo\"\n                option-store=\"company\"\n                label=\"companyTo_name\"\n                @formUpdated=\"updateValue\"\n              />\n            </div>\n          </div>\n          <div class=\"row\">\n            <div class=\"col-md-6\">\n              <form-select\n                :item=\"item\"\n                :errors=\"errors\"\n                property=\"type\"\n                option-store=\"invoiceType\"\n                label=\"type_name\"\n                @formUpdated=\"updateValue\"\n              />\n            </div>\n            <div class=\"col-md-6\">\n              <form-select\n                :item=\"item\"\n                :errors=\"errors\"\n                property=\"currency\"\n                option-store=\"currency\"\n                label=\"currency_name\"\n                @formUpdated=\"updateValue\"\n              />\n            </div>\n          </div>\n          <div class=\"row\">\n            <div class=\"col-md-6\">\n              <form-select\n                :item=\"item\"\n                :errors=\"errors\"\n                property=\"agreement\"\n                option-store=\"document\"\n                label=\"agreement_name\"\n                @formUpdated=\"updateValue\"\n              />\n            </div>\n            <div class=\"col-md-6\">\n              <form-select\n                :item=\"item\"\n                :errors=\"errors\"\n                property=\"language\"\n                option-store=\"language\"\n                label=\"language_name\"\n                @formUpdated=\"updateValue\"\n              />\n            </div>\n          </div>\n          <div class=\"row\">\n            <div class=\"col-md-6\">\n              <form-date-picker\n                :item=\"item\"\n                :errors=\"errors\"\n                property=\"dateOfInvoice\"\n                label=\"dateOfInvoice\"\n                @formUpdated=\"updateValue\"\n              />\n            </div>\n            <div class=\"col-md-6\">\n              <form-input\n                :item=\"item\"\n                :errors=\"errors\"\n                property=\"maturity\"\n                label=\"maturity\"\n                @formUpdated=\"updateValue\"\n              />\n            </div>\n          </div>\n          <div class=\"row\">\n            <div class=\"col-md-6\">\n              <form-date-picker\n                :item=\"item\"\n                :errors=\"errors\"\n                property=\"dateOfSale\"\n                label=\"dateOfSale\"\n                @formUpdated=\"updateValue\"\n              />\n            </div>\n            <div class=\"col-md-6\">\n              <form-select\n                :item=\"item\"\n                :errors=\"errors\"\n                property=\"orderHeader\"\n                option-store=\"orderHeader\"\n                label=\"orderHeader_number\"\n                @formUpdated=\"updateValue\"\n              />\n            </div>\n          </div>\n          <div class=\"row\">\n            <div class=\"col-md-6\">\n              <form-checkbox\n                :item=\"item\"\n                :errors=\"errors\"\n                property=\"isActive\"\n                label=\"isActive\"\n                @formUpdated=\"updateValue\"\n              />\n            </div>\n            <div class=\"col-md-6\" />\n          </div>\n        </div>\n      </div>\n      <div class=\"box box-primary\">\n        <div class=\"box-header with-border\">\n          <h3 class=\"box-title\">\n            {{ $t('tabs_lines') }}\n          </h3>\n        </div>\n        <div class=\"box-body\">\n          <button type=\"button\" class=\"btn btn-info\" @click.prevent=\"addLine()\">\n            <span class=\"fa fa-plus\" aria-hidden=\"true\" />\n            <span>{{ $t('invoice_line_add') }}</span>\n          </button>\n          <table class=\"table table-striped\">\n            <thead>\n              <tr>\n                <th>{{ $t('product_name') }}</th>\n                <th>{{ $t('name') }}</th>\n                <th>{{ $t('vat_name') }}</th>\n                <th>{{ $t('unitPriceNetto') }}</th>\n                <th>{{ $t('unitsCount') }}</th>\n                <th width=\"60px;\" />\n              </tr>\n            </thead>\n            <tbody>\n              <tr v-for=\"line in item.lines\" :key=\"line.id\">\n                <td>\n                  <form-select\n                    :item=\"line\"\n                    :errors=\"errors\"\n                    property=\"product\"\n                    option-store=\"product\"\n                    label=\"product_name\"\n                    @formUpdated=\"updateValue\"\n                  />\n                </td>\n                <td>\n                  <form-input\n                    :item=\"line\"\n                    :errors=\"errors\"\n                    property=\"name\"\n                    label=\"name\"\n                    @formUpdated=\"updateValue\"\n                  />\n                </td>\n                <td>\n                  <form-select\n                    :item=\"line\"\n                    :errors=\"errors\"\n                    property=\"vat\"\n                    option-store=\"vat\"\n                    label=\"vat_name\"\n                    @formUpdated=\"updateValue\"\n                  />\n                </td>\n                <td>\n                  <form-number\n                    :item=\"line\"\n                    :errors=\"errors\"\n                    property=\"unitPriceNetto\"\n                    label=\"unitPriceNetto\"\n                    @formUpdated=\"updateValue\"\n                  />\n                </td>\n                <td>\n                  <form-number\n                    :item=\"line\"\n                    :errors=\"errors\"\n                    property=\"unitsCount\"\n                    label=\"unitsCount\"\n                    @formUpdated=\"updateValue\"\n                  />\n                </td>\n                <td>\n                  <button\n                    type=\"button\"\n                    class=\"btn btn-info\"\n                    @click.prevent=\"removeLine(line)\"\n                  >\n                    <span class=\"fa fa-remove\" aria-hidden=\"true\" />\n                    <span class=\"sr-only\">{{ $t('delete') }}</span>\n                  </button>\n                </td>\n              </tr>\n            </tbody>\n          </table>\n        </div>\n      </div>\n      <item-edit-actions\n        :item=\"item\"\n        entity=\"InvoiceHeader\"\n        path=\"invoice_header\"\n      />\n    </section>\n  </form>\n</template>\n\n<script>\nimport { mapActions, mapGetters } from 'vuex'\nimport ItemEditActions from '../../components/layout/ItemEditActions'\nimport ItemErrors from '../../components/layout/errors/ItemErrors'\n\nexport default {\n  components: {\n    ItemErrors,\n    ItemEditActions\n  },\n  props: {\n    handleSubmit: {\n      type: Function,\n      required: true\n    },\n    item: {\n      type: Object,\n      required: true\n    }\n  },\n  computed: {\n    ...mapGetters({\n      errors: 'invoiceHeader/errors'\n    })\n  },\n  beforeDestroy() {\n    this.reset()\n  },\n  methods: {\n    ...mapActions({\n      reset: 'invoiceHeader/reset'\n    }),\n    updateValue(property, value) {\n      this.$store.commit('invoiceHeader/INVOICE_HEADER_UPDATE_ITEM', {\n        [property]: value\n      })\n    },\n    addLine() {\n      this.item.lines.push({ uuid: Date.now() })\n    },\n    removeLine(line) {\n      this.item.lines = this.item.lines.filter(el => {\n        if (!line.id) {\n          line.id = line.uuid\n        }\n\n        return line.id !== el.id\n      })\n    },\n    productSelected(line) {\n      if (!line.name) {\n        line.name = line.product.name\n      }\n\n      if (!line.vat) {\n        line.vat = line.product.vat\n      }\n\n      if (!line.unitsCount) {\n        line.unitsCount = 1\n      }\n\n      // TODO: add prices to product\n      if (!line.unitPriceNetto) {\n        // line.unitPriceNetto = line.product.price\n      }\n    }\n  }\n}\n</script>\n"],"sourceRoot":"src/pages/invoice_header"}]}