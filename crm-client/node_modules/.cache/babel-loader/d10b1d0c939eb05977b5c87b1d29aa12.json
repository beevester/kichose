,CAAN;AAEA,SAAOH,KAAK,CACTiC,GADI,WAEAhC,QAFA,cAEYF,SAAS,CAACI,SAAS,CAACI,WAAV,EAAD,CAFrB,cAEkDgB,KAAK,CAACE,IAAN,CAAWrB,EAF7D,GAGHmB,KAAK,CAACE,IAHH,EAKJjB,IALI,CAKC,UAAAC,QAAQ;AAAA,WAAIA,QAAQ,CAACC,IAAb;AAAA,GALT,EAMJC,KANI,CAME,UAAAC,CAAC,EAAI;AAAA,QACFF,IADE,GACOE,CAAC,CAACH,QADT,CACFC,IADE;;AAGV,QAAIA,IAAI,CAACgB,UAAT,EAAqB;AACnB,UAAMC,MAAM,GAAG,EAAf;AAEAjB,MAAAA,IAAI,CAACgB,UAAL,CAAgBP,GAAhB,CAAoB,UAAAS,SAAS,EAAI;AAC/BX,QAAAA,MAAM,CAACY,MAAP,CAAcF,MAAd,sBAAyBC,SAAS,CAACE,YAAnC,EAAkDF,SAAS,CAACf,OAA5D;AACD,OAFD;AAIAR,MAAAA,MAAM,WAAIF,SAAJ,kBAA4BwB,MAA5B,CAAN;AACD;;AAED,QAAMI,KAAK,GAAGrB,IAAI,CAAC,mBAAD,CAAJ,GACVA,IAAI,CAAC,mBAAD,CADM,GAEVA,IAAI,CAACG,OAFT;AAGAR,IAAAA,MAAM,WAAIF,SAAJ,iBAA2B4B,KAA3B,CAAN;AAEA,UAAMrB,IAAN;AACD,GAzBI,CAAP;AA0BD,CA7BM;AA+BP,OAAO,IAAMwB,MAAM,GAAG,SAATA,MAAS,QAAa/B,SAAb,EAAwBsB,IAAxB;AAAA,MAAGpB,MAAH,SAAGA,MAAH;AAAA,SACpBL,KAAK,CACFmC,MADH,WACalC,QADb,cACyBF,SAAS,CAACI,SAAS,CAACI,WAAV,EAAD,CADlC,cAC+DkB,IAAI,CAACrB,EADpE,GAEGO,KAFH,CAES,UAAAC,CAAC,EAAI;AACV,QAAMC,OAAO,GACXD,CAAC,CAACH,QAAF,CAAW2B,MAAX,KAAsB,GAAtB,GACI,oDADJ,GAEIxB,CAAC,CAACC,OAHR;AAKAR,IAAAA,MAAM,WAAIF,SAAJ,iBAA2BU,OAA3B,CAAN;AAEA,UAAMD,CAAN;AACD,GAXH,CADoB;AAAA,CAAf;AAcP,OAAO,IAAMyB,KAAK,GAAG,SAARA,KAAQ,QAAalC,SAAb,EAA2B;AAAA,MAAxBE,MAAwB,SAAxBA,MAAwB;AAC9CA,EAAAA,MAAM,WAAIF,SAAJ,YAAN;AACD,CAFM","sourcesContent":["import pluralize from 'pluralize'\nimport axios from '../interceptor'\nimport { API_HOST } from '../config/_entrypoint'\n\nexport const getItem = ({ commit }, namespace, id) => {\n  commit(`${namespace}_SET_ERROR`, null)\n\n  return axios\n    .get(`${API_HOST}/${pluralize(namespace.toLowerCase())}/${id}`)\n    .then(response => response.data)\n    .then(data => {\n      commit(`${namespace}_SET_ITEM`, data)\n\n      return data\n    })\n    .catch(e => {\n      commit(`${namespace}_SET_ERROR`, e.message)\n    })\n}\n\nexport const getItems = ({ commit }, namespace, query) => {\n  let url = `/${pluralize(namespace.toLowerCase())}`\n\n  if (query) {\n    url += `?${Object.keys(query)\n      .map(key => `${key}=${query[key]}`)\n      .join('&')}`\n  }\n\n  commit(`${namespace}_SET_ERROR`, null)\n\n  return axios\n    .get(API_HOST + url)\n    .then(response => response.data)\n    .then(data => {\n      commit(`${namespace}_SET_ITEMS`, data['hydra:member'])\n\n      return data['hydra:member']\n    })\n    .catch(e => {\n      commit(`${namespace}_SET_ERROR`, e.message)\n    })\n}\n\nexport const create = ({ commit, state }, namespace) => {\n  commit(`${namespace}_SET_ERROR`, null)\n\n  return axios\n    .post(`${API_HOST}/${pluralize(namespace.toLowerCase())}`, state.item)\n    .then(response => response.data)\n    .catch(e => {\n      const { data } = e.response\n\n      if (data.violations) {\n        const errors = {}\n\n        data.violations.map(violation => {\n          Object.assign(errors, { [violation.propertyPath]: violation.message })\n        })\n\n        commit(`${namespace}_SET_ERRORS`, errors)\n      }\n\n      const error = data['hydra:description']\n        ? data['hydra:description']\n        : data.message\n      commit(`${namespace}_SET_ERROR`, error)\n\n      throw data\n    })\n}\n\nexport const update = ({ commit, state }, namespace) => {\n  commit(`${namespace}_SET_ERROR`, null)\n\n  return axios\n    .put(\n      `${API_HOST}/${pluralize(namespace.toLowerCase())}/${state.item.id}`,\n      state.item\n    )\n    .then(response => response.data)\n    .catch(e => {\n      const { data } = e.response\n\n      if (data.violations) {\n        const errors = {}\n\n        data.violations.map(violation => {\n          Object.assign(errors, { [violation.propertyPath]: violation.message })\n        })\n\n        commit(`${namespace}_SET_ERRORS`, errors)\n      }\n\n      const error = data['hydra:description']\n        ? data['hydra:description']\n        : data.message\n      commit(`${namespace}_SET_ERROR`, error)\n\n      throw data\n    })\n}\n\nexport const remove = ({ commit }, namespace, item) =>\n  axios\n    .delete(`${API_HOST}/${pluralize(namespace.toLowerCase())}/${item.id}`)\n    .catch(e => {\n      const message =\n        e.response.status === 409\n          ? 'Not possible to remove. This record has relations.'\n          : e.message\n\n      commit(`${namespace}_SET_ERROR`, message)\n\n      throw e\n    })\n\nexport const reset = ({ commit }, namespace) => {\n  commit(`${namespace}_RESET`)\n}\n"]}]}