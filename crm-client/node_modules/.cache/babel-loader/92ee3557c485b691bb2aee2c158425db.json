fault: false\n    },\n    tasks: {\n      type: Array,\n      default: () => []\n    }\n  },\n  data() {\n    return {\n      headers: [\n        { text: this.$t('id'), value: 'id' },\n        { text: this.$t('name'), value: 'name' },\n        {\n          text: this.$t('client'),\n          value: 'project.name',\n          type: 'object',\n          sortable: false\n        },\n        {\n          text: this.$t('assignee_name'),\n          value: 'assignee.name',\n          type: 'object',\n          sortable: false\n        },\n        {\n          text: this.$t('status_name'),\n          value: 'status.name',\n          type: 'object',\n          sortable: false\n        },\n        {\n          text: this.$t('deadline'),\n          value: 'deadline',\n          type: 'datetime',\n          sortable: false\n        },\n        {\n          text: this.$t('createdAt'),\n          value: 'createdAt',\n          type: 'datetime',\n          sortable: false\n        },\n        { text: '', value: 'actions', sortable: false }\n      ]\n    }\n  },\n  methods: {\n    goto(task) {\n      this.dialogClose()\n      this.$router.push({ name: 'TaskShow', params: { id: task.id } })\n    },\n    dialogClose() {\n      this.$emit('dialog-close')\n    }\n  }\n}\n</script>\n"],"sourceRoot":"src/components/layout"}]}